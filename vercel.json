{
  "version": 2,
  "name": "zicount",
  "functions": {
    "app/api/analyze/route.js": {
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/_next/:path*",
      "dest": "/_next/:path*"
    },
    {
      "src": "/static/:path*",
      "dest": "/static/:path*"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET, POST, PUT, DELETE, OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization" }
      ]
    }
  ]
}

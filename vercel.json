{
  "version": 2,
  "functions": {
    "pages/api/analyze.js": {
      "maxDuration": 30
    },
    "pages/api/upload.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/_next/static/(.*)", "dest": "/_next/static/$1" },
    { "src": "/(.*)", "dest": "/" }
  ]
}
